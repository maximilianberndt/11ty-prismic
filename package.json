		"html-minifier": "^4.0.0",
		"mini-css-extract-plugin": "^2.6.0",
		"npm-run-all": "^4.1.5",
		"path": "^0.12.7",
		"postcss-loader": "^6.2.1",
		"postcss-preset-env": "^7.4.2",
		"sass": "^1.49.9",
		"sass-loader": "^12.6.0",
		"terser-webpack-plugin": "^5.3.1",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
