{
	"name": "eleventy-intro",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"dependencies": {
		"clean-css": "^5.2.4",
		"eleventy-plugin-prismic": "^0.2.1"
	},
	"scripts": {
		"dev": "cross-env NODE_ENV=development npm-run-all -l --parallel dev:*",
